    <h1 class=text>MENU</h1>
    <!-- Copy in a card from Bootstrap -->
    <!-- Put the card in an EJS for loop and dynamically plug in your values -->
<div class="container">
  <div class="row">
    <% for (let i = 0; i < menu.length; i++) { %>
      <div class="card" style="width: 18rem;">
        <img src="<%= menu[i].picture%>" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= menu[i].name %></h5>
          <p class="card-text"><%= menu[i].description%> 
      <p></p>      (Dietary-<%= menu[i].dietary%> Allergies-<%= menu[i].allergies%>) Rating - <%= menu[i].rating%>/10</p>
         <center> <a href="#" class="btn btn-primary">$<%= menu[i].price %></a></center>
        </div>
      </div>  
          <% } %>
          </div>
          </div>
  
      <script src="script.js"></script>
    <!-- Link Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

